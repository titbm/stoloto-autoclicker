{
  "manifest_version": 3,
  "name": "Столото Автокликер",
  "version": "1.0.0",
  "description": "Автоматический поиск билетов с заданными числами на сайте Столото",
  "author": "titbm",
  "homepage_url": "https://github.com/titbm/stoloto-autoclicker",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.stoloto.ru/*"],
      "js": ["content.js"]
    }
  ]
}
