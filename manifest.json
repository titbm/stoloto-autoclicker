{
  "manifest_version": 3,
  "name": "Столото Автокликер",
  "version": "2.0.0",
  "description": "Автоматический поиск билетов с заданными числами на сайте Столото",
  "key": "",
  "author": "titbm",
  "homepage_url": "https://github.com/titbm/stoloto-autoclicker",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "sidePanel"
  ],
  "host_permissions": [
    "https://www.stoloto.ru/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.stoloto.ru/ruslotto/game*",
        "https://www.stoloto.ru/auth/*"
      ],
      "js": [
        "src/content-main.js"
      ],
      "run_at": "document_start",
      "world": "MAIN"
    },
    {
      "matches": [
        "https://www.stoloto.ru/ruslotto/game*",
        "https://www.stoloto.ru/auth/*"
      ],
      "js": [
        "src/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_title": "Открыть панель поиска"
  },
  "side_panel": {
    "default_path": "src/ui/sidepanel/sidepanel.html"
  }
}
